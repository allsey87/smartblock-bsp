./ftx_prog --cbus[0] BCD_Charger# --cbus[1] PWREN --cbus[2] SLEEP --cbus[3] TxRxLED --max-bus-power 500 --self-powered off

sudo ./ftx_prog --cbus 0 BCD_Charger# --cbus 1 PWREN --cbus 2 SLEEP --cbus 3 TxRxLED --max-bus-power 500 --self-powered off

